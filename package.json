{
  "type": "module",
  "name": "@daldalso/tailwind-base",
  "version": "0.9.2-4",
  "description": "Let you exploit Tailwind in React more powerful",
  "repository": {
    "url": "git+https://github.com/JJoriping/tailwind-base.git"
  },
  "author": "JJoriping",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "test": "node --test"
  },
  "files": [
    "/LICENSE",
    "/README.md",
    "/react.d.ts",
    "/dist/"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./transformer": {
      "default": "./dist/transformer.js"
    },
    "./lib": {
      "default": "./dist/lib/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "default": "./dist/jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime/index.d.ts",
      "default": "./dist/jsx-dev-runtime/index.js"
    }
  },
  "bin": "./dist/bin.js",
  "devDependencies": {
    "@daldalso/eslint-plugin": "^1.2.3",
    "@types/node": "^22.4.2",
    "@types/react": "^18.3.3",
    "eslint": "^9.9.0",
    "postcss": "^8.4.41",
    "tailwind-merge": "^2.5.2",
    "tinybench": "^2.9.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.4.0",
  "dependencies": {
    "@daldalso/logger": "^0.9.3-0",
    "tailwindcss": "^3.4.10"
  },
  "peerDependencies": {
    "react": "^18"
  }
}
